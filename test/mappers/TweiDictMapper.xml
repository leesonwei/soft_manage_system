			separator=",">
			(#{item.typeId},#{item.dictId},#{item.dictName},#{item.dictShortName},#{item.dictFullName},#{item.createBy},#{item.createAt},#{item.updateBy},#{item.updateAt},#{item.flag},#{item.checkBy},#{item.checkAt},#{item.isUsed},#{item.memo},#{item.pycode},#{item.dataVersion})
		</foreach>
	</insert>
	

	<!-- 通过辅助工具Assist中的条件将TweiDict对应的数据库表的数据删除 -->
	<delete id="deleteTweiDictByAssist" parameterType="com.common.Assist">
		delete from twei_dict
		<if test="require!=null">
			<include refid="assist" />
		</if>
	</delete>
	
	
	
	<!-- 通过辅助工具Assist中的条件将TweiDict中的数据更新到数据库对应的表中,包括值为null的数据 -->
	<update id="updateTweiDict" parameterType="map">
		update twei_dict
		<set>
			type_id = #{enti.typeId},
			dict_id = #{enti.dictId},
			dict_name = #{enti.dictName},
			dict_short_name = #{enti.dictShortName},
			dict_full_name = #{enti.dictFullName},
			create_by = #{enti.createBy},
			create_at = #{enti.createAt},
			update_by = #{enti.updateBy},
			update_at = #{enti.updateAt},
			flag = #{enti.flag},
			check_by = #{enti.checkBy},
			check_at = #{enti.checkAt},
			is_used = #{enti.isUsed},
			memo = #{enti.memo},
			pycode = #{enti.pycode},
			data_version = #{enti.dataVersion},
		</set>
		<if test="assist.require!=null">
			<include refid="updateAssist" />
		</if>
	</update>

