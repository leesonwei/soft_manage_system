			<if test="enti.typeId != null">type_id = #{enti.typeId},</if>
			<if test="enti.dictId != null">dict_id = #{enti.dictId},</if>
			<if test="enti.dictName != null">dict_name = #{enti.dictName},</if>
			<if test="enti.dictShortName != null">dict_short_name = #{enti.dictShortName},</if>
			<if test="enti.dictFullName != null">dict_full_name = #{enti.dictFullName},</if>
			<if test="enti.createBy != null">create_by = #{enti.createBy},</if>
			<if test="enti.createAt != null">create_at = #{enti.createAt},</if>
			<if test="enti.updateBy != null">update_by = #{enti.updateBy},</if>
			<if test="enti.updateAt != null">update_at = #{enti.updateAt},</if>
			<if test="enti.flag != null">flag = #{enti.flag},</if>
			<if test="enti.checkBy != null">check_by = #{enti.checkBy},</if>
			<if test="enti.checkAt != null">check_at = #{enti.checkAt},</if>
